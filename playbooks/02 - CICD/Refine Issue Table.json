            "description": null,
            "arguments": {
                "params": {
                    "value": "<!DOCTYPE html>\n<html>\n<head>\n    <style>\n        table.scrolldown {\n            width: 100%;\n            \/* border-collapse: collapse; *\/\n            border-spacing: 0;\n            border: 2px solid black;\n        }\n        \/* To display the block as level element *\/\n        table.scrolldown tbody,\n        table.scrolldown thead {\n            display: table-header-group;\n        }\n        thead tr th {\n            height: 40px;\n            text-align: center;\n            \/* line-height: 40px;*\/\n        }\n        table.scrolldown tbody {\n            \/* Set the height of table body *\/\n            height: 50px;\n            \/* Set vertical scroll *\/\n            overflow-y: auto;\n            \/* Hide the horizontal scroll *\/\n            overflow-x: hidden;\n        }\n        tbody {\n            border-top: 2px solid black;\n        }\n        tbody td,\n        thead th {\n            width: 200px;\n            border-right: 2px solid black;\n        }\n        td {\n            text-align: center;\n        }\n    <\/style>\n<\/head>\n\n<body>\n    <table class=\"scrolldown\">\n        <!-- Table head content -->\n        <thead>\n            <tr>\n                <th>CR-No<\/th>\n               <th>Repository<\/th>\n               <th>Base Branch<\/th>\n                <th>Type<\/th>\n                <th>State<\/th>\n                <th>Summary<\/th>\n                <th>Description<\/th>\n                <th>Assignee Username<\/th>\n                <th>Reporter Username<\/th>\n                <th>Pull Request<\/th>\n                <th>PR Status<\/th>\n                <th>PR Reviewers<\/th>\n            <\/tr>\n        <\/thead>\n        <!-- Table body content -->\n        <tbody>\n            {% for item in vars.issues_json %}\n            <tr>\n                <td>{{item.cRNo}}<\/td>\n                <td>{{item.repo_url}}<\/td>\n                <td>{{item.baseBranch}}<\/td>\n                <td>{{item.type}}<\/td>\n                <td>{{item.state}}<\/td>\n                <td>{{item.summary}}<\/td>\n                <td>{{item.description}}<\/td>\n                <td>{{item.assigneeUsername}}<\/td>\n                <td>{{item.reporterUsername}}<\/td>\n                <td>{{item.pullRequest}}<\/td>\n                <td>{{item.pRStatus}}<\/td>\n                <td>{{item.pRReviewers}}<\/td>\n            <\/tr>\n            {% endfor %}\n        <\/tbody>\n    <\/table>\n    <body>\n<\/html>"
                },
